#include<bits/stdc++.h>
using namespace std;
int main()
{
	int b[3][3];
	for(int i=0;i<3;i++)
	{
		for(int j=0;j<3;j++)
		cin >> b[i][j];
	}
	int a[3][3] ;a[0][0] = (b[0][0]+b[0][1]+b[1][0]+1)%2;a[2][2]=(b[2][2]+b[2][1]+b[1][2]+1)%2;a[2][0]=(b[2][0]+b[1][0]+b[2][1]+1)%2;
	a[0][2]=(b[0][2]+b[0][1]+b[1][2]+1)%2;
	a[0][1] = (b[0][1]+b[0][0]+b[0][2]+b[1][1]+1)%2;
	a[2][1] = (b[2][1]+b[1][1]+b[2][0]+b[2][2]+1)%2;
	a[1][0] = (b[1][0]+b[0][0]+b[2][0]+b[1][1]+1)%2;
	a[1][2] = (b[1][2]+b[0][2]+b[2][2]+b[1][1]+1)%2;
	a[1][1] = (b[1][1]+b[0][1]+b[1][0]+b[1][2]+b[2][1]+1)%2;
	for(int i=0;i<3;i++)
	{
		for(int j=0;j<3;j++)
		cout << a[i][j];
		cout << endl;
	}
}
